{
  "name": "linkedin-jobs-match",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron.js",
  "homepage": "./",
  "dependencies": {
    "@ant-design/icons": "^4.0.6",
    "antd": "^4.1.4",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.4.1",
    "concurrently": "^5.2.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "express": "^4.17.1",
    "global": "^4.4.0",
    "puppeteer": "^2.1.1",
    "puppeteer-core": "^2.1.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.1.1",
    "react-tooltip": "^4.2.5",
    "wait-on": "^4.0.2",
    "electron-is-dev": "^1.2.0"
  },
  "scripts": {
    "server": "node index.js",
    "start": "react-app-rewired start",
    "build": "react-scripts build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "dev": "concurrently --kill-others \"react-app-rewired start\" \"npm run server\"",
    "electron-dev": "concurrently \"cross-env BROWSER=none yarn start\" \"npm run server\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-pack": "electron-builder -c.extraMetadata.main=build/electron.js",
    "preelectron-pack": "react-app-rewired build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "react-app-rewired": "^2.1.3",
    "workbox-webpack-plugin": "^4.3.1",
    "electron": "^8.2.5",
    "electron-builder": "^22.6.0"
  }
}
